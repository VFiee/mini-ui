{
  "name": "@vyron/mini-ui",
  "version": "1.0.0-alpha.3",
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "author": "vyron",
  "license": "MIT",
  "homepage": "https://github.com/VFiee",
  "repository": {
    "type": "git",
    "url": "https://github.com/VFiee"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "example": "cd example && yarn dev",
    "build:dev": "yarn clean && rollup -c --watch",
    "build": "yarn clean && rollup -c --environment BUILD:production",
    "reinstall": "rm -rf node_modules yarn.lock yarn-error.log && yarn install",
    "release": "node script/release",
    "lint": "eslint packages --max-warnings 0",
    "lint:style": "stylelint packages --fix",
    "prettier": "prettier packages --write",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0 && prettier CHANGELOG.md --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && git add .",
      "commit-msg": "node script/verifyCommit.js"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn prettier",
      "yarn lint"
    ],
    "*.{less}": [
      "yarn prettier",
      "yarn lint:style"
    ],
    "*.{md}": "yarn prettier"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tarojs/components": "^3.0.11",
    "@tarojs/taro": "3.0.12",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.49",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "chalk": "^4.1.0",
    "enquirer": "^2.3.6",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-taro": "^3.0.14",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "1.6.1",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "lint-staged": "^10.4.2",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-sizes": "^1.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "semver": "^7.3.2",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.kebabcase": "^4.1.1",
    "qs": "^6.9.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
